{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "alaouy/youtube": "^2.1",
        "barryvdh/laravel-debugbar": "2.4.*",
        "caffeinated/modules": "^4.2",
        "chumper/zipper": "1.0.x",
        "cviebrock/eloquent-sluggable": "^4.2",
        "cviebrock/eloquent-taggable": "^2.1",
        "davejamesmiller/laravel-breadcrumbs": "3.*",
        "doctrine/dbal": "^2.5",
        "gmopx/laravel-owm": "^0.1.2",
        "greggilbert/recaptcha": "^2.2",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.5",
        "junityco/laravel-hashids": "^1.5",
        "laravel/framework": "5.4.30",
        "laravel/socialite": "3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maatwebsite/excel": "^2.1",
        "mcamara/laravel-localization": "^1.3",
        "roumen/feed": "v2.10.5",
        "silber/bouncer": "v1.0.0-beta.2",
        "spatie/laravel-image-optimizer": "^1.1",
        "spatie/laravel-medialibrary": "^5.12",
        "unisharp/laravel-filemanager": "~1.8",
        "willvincent/feeds": "1.1.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "phpunit/php-code-coverage": "^4.0.4"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Library/"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": false
    }
}
